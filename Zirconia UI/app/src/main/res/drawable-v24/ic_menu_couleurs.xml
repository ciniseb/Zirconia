<vector android:height="50dp"
    android:viewportHeight="200"
    android:viewportWidth="200"
    android:width="50dp"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <path android:pathData="
            M 100,100
            m -80,0
            a 80,80 0 1,0 160,0
            a 80,80 0 1,0 -160,0">
        <aapt:attr name="android:fillColor">
            <gradient
                android:centerX="100"
                android:centerY="100"
                android:gradientRadius="100"
                android:type="radial">
                <item
                    android:color="#ffffff"
                    android:offset="0.0" />
                <item
                    android:color="#999999"
                    android:offset="1.0" />
            </gradient>
        </aapt:attr>
    </path>
    <path android:fillColor="#00000000"
        android:pathData="
            M 100,100
            m -80,0
            a 80,80 0 1,0 160,0
            a 80,80 0 1,0 -160,0"
        android:strokeColor="@android:color/black" android:strokeWidth="15"/>
</vector>